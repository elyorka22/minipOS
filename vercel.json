{
  "version": 2,
  "buildCommand": "cd frontend && node replace-env.js $VITE_API_URL || echo 'No build needed'",
  "outputDirectory": "frontend",
  "installCommand": "echo 'No install needed'",
  "root": "frontend",
  "framework": null,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}

